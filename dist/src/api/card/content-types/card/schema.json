{
    "kind": "collectionType",
    "collectionName": "cards",
    "info": {
        "singularName": "card",
        "pluralName": "cards",
        "displayName": "Card"
    },
    "options": {
        "draftAndPublish": false
    },
    "pluginOptions": {},
    "attributes": {
        "title": {
            "type": "string",
            "required": true
        },
        "description": {
            "type": "text"
        },
        "order": {
            "type": "integer",
            "required": true,
            "default": 0
        },
        "list": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::list.list",
            "inversedBy": "cards"
        },
        "users_permissions_user": {
            "type": "relation",
            "relation": "oneToOne",
            "target": "plugin::users-permissions.user"
        },
        "labels": {
            "type": "relation",
            "relation": "manyToMany",
            "target": "api::label.label",
            "mappedBy": "cards"
        },
        "dueDate": {
            "type": "date"
        }
    }
}
